<div class='wrapper'>
  <h2>Review UpDog</h2>
  <%= form_for @review do |f| %>
    <%= f.label :rating %>
    <%= f.text_field :rating, 'data-ratable' => true %>
    <%= f.label :comment %>
    <%= f.text_area :comment %>
    <%= f.submit class: 'btn-create' %>
  <% end %>
</div>
<script type="text/javascript" src='https://unpkg.com/ratable@1.0.4'></script>
